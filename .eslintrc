{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": 0,
    "import/extensions": 0,
    "object-curly-newline": 0,
    "radix": 0,
    "no-console": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-use-before-define": "off",
    "no-shadow": "off",
    "no-unused-vars": 1,
    "react/no-children-prop": 0
  }
}
